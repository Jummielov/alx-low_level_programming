#include <main.h>
/**
 * main - This prints -putchar using putchar
 *
 * Return: 0
 */
int main(void)
{
        _putchar('_');
        _putchar('p');
        _putchar('u');
        _putchar('t');
        _putchar('c');
        _putchar('h');
        _putchar('a');
        _putchar('r');
        -putchar('\n');
        char str[] = "_putchar";
        int ch;

        for (ch = 0; ch < 8; ++ch)
                _putchar(str[ch]);
return (0);
}
